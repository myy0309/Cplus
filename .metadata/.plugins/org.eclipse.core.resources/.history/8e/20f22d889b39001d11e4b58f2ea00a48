#include "Student.h"
#include <iostream>
using namespace std;

Student::Student() {
	this->age = 18;
	this->weight = 50.0
}
Student::Student(int age) : age(age){
}

Student::~Student() {
}

void Student::showAge(){
	cout << this->age << endl;
}
