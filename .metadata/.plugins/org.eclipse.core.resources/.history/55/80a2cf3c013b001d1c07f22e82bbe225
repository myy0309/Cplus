Father::Father(int age, float money) : age(age), money(money){
}

Father::~Father() {
}


void Father::setAge(int age){
	this->age = age;
}

void Father::printAge(){
	cout << "Father's age is " << age << endl;
}
void Father::printAge(int i){
	cout << "My age plus i is " << age+i << endl;
}
